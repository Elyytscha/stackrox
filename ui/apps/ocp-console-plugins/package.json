{
    "name": "acs-ocp-console-plugins",
    "version": "0.0.1",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/stackrox/stackrox.git",
        "directory": "ui/apps/ocp-console-plugins"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "yarn clean && NODE_ENV=production yarn ts-node node_modules/.bin/webpack",
        "build-dev": "yarn clean && yarn ts-node node_modules/.bin/webpack",
        "image:build": "docker build -t quay.io/rh_ee_dvail/acs-violations-plugin:latest .",
        "image:run": "docker run -it --rm -p 9090:80 quay.io/rh_ee_dvail/acs-violations-plugin:latest",
        "image:push": "docker push quay.io/rh_ee_dvail/acs-violations-plugin:latest",
        "start": "yarn ts-node node_modules/.bin/webpack serve",
        "start-console": "./start-console.sh",
        "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@openshift-console/dynamic-plugin-sdk": "0.0.3",
        "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.6",
        "@patternfly/react-core": "4.175.4",
        "@types/node": "^17.0.16",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/webpack-dev-server": "^4.7.2",
        "prettier": "^2.7.1",
        "react-test-renderer": "^17.0.2",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.8",
        "ts-node": "^10.7.0",
        "typescript": "^4.7.4",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    },
    "consolePlugin": {
        "name": "acs-ocp-console-plugins",
        "version": "0.0.1",
        "displayName": "Advanced Cluster Security Plugins",
        "description": "ACS -> OCP Plugins",
        "exposedModules": {
            "Violations": "./modules/Violations"
        },
        "dependencies": {
            "@console/pluginAPI": "*"
        }
    }
}
