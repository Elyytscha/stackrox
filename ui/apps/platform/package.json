    "scripts": {
        "clean": "rm -rf ./src/index.css ./cypress/test-results",
        "start": "yarn tailwind && HTTPS=true react-scripts start",
        "start:watch": "npm-run-all --parallel watch:tailwind watch:react",
        "start:coverage": "yarn tailwind && HTTPS=true react-scripts -r @cypress/instrument-cra start",
        "build": "yarn tailwind && CI=false GENERATE_SOURCEMAP=false react-scripts build",
        "test": "JEST_JUNIT_UNIQUE_OUTPUT_NAME=true JEST_JUNIT_OUTPUT_DIR=${TEST_RESULTS_OUTPUT_DIR:-test-results}/reports react-scripts test --coverage --reporters=default --reporters=jest-junit",
        "test-watch": "react-scripts test",
        "lint": "yarn tailwind && NODE_PATH=src eslint --quiet --ext .js,.ts,.tsx src cypress tailwind.config.js .prettierrc.js .eslintrc.js",
        "cypress-open": "./scripts/cypress.sh open --config defaultCommandTimeout=8000",
        "cypress-spec": "./scripts/cypress.sh run --spec",
        "cypress-demo-open": "./scripts/cypress.sh open --config integrationFolder=cypress/demo",
        "test-e2e": "./scripts/cypress.sh run --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha.config.js",
        "test-e2e:coverage": "CYPRESS_COLLECT_COVERAGE=true yarn test-e2e",
        "test-e2e-local": "./scripts/cypress.sh run",
        "test-e2e-local:coverage": "CYPRESS_COLLECT_COVERAGE=true yarn test-e2e-local",
        "test-e2e-demo-local": "./scripts/cypress.sh run --config integrationFolder=cypress/demo",
        "test-e2e-demo": "./scripts/cypress.sh run --config integrationFolder=cypress/demo --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha.config.js",
        "storybook": "start-storybook",
        "build-storybook": "build-storybook -c .storybook",
        "tailwind": "tailwind build ./src/app.tw.css -o ./src/index.css",
        "watch:tailwind": "postcss ./src/app.tw.css -o ./src/index.css -w",
        "watch:react": "HTTPS=true react-scripts start",
        "generate-graphql-possible-types": "ROX_AUTH_TOKEN=$(./scripts/get-auth-token.sh) node scripts/generate-graphql-possible-types.js",
        "eject": "react-scripts eject"
    },
