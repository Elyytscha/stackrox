{
    "name": "@stackrox/ui-components",
    "version": "0.1.0",
    "description": "StackRox library of UI (React) components",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/stackrox/rox.git",
        "directory": "ui/packages/ui-components"
    },
    "license": "UNLICENSED",
    "files": [
        "lib/**/*",
        "src/**/*",
        "!src/**/*.test.ts",
        "!src/**/*.test.tsx",
        "!src/**/*.stories.tsx"
    ],
    "scripts": {
        "clean": "rm -rf lib test-results coverage",
        "start:tailwind": "postcss -w ./src/index.tw.css -o ./lib/ui-components.css",
        "start:ts": "tsc --watch",
        "start": "npm-run-all -p start:*",
        "build:tailwind": "postcss ./src/index.tw.css -o ./lib/ui-components.css",
        "build:ts": "tsc --build",
        "build": "npm-run-all build:*",
        "test": "JEST_JUNIT_UNIQUE_OUTPUT_NAME=true JEST_JUNIT_OUTPUT_DIR=${TEST_RESULTS_OUTPUT_DIR:-test-results}/reports jest --coverage --reporters=default --reporters=jest-junit",
        "test:watch": "jest --watch-all",
        "lint:non-src": "prettier --check '**/*.{md,css,json}'",
        "lint:src": "eslint --ext .js,.jsx,.ts,.tsx ./",
        "lint": "npm-run-all lint:*",
        "lint-fix:non-src": "prettier --write '**/*.{md,css,json}'",
        "lint-fix:src": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
        "lint-fix": "npm-run-all lint-fix:*",
        "prepublishOnly": "yarn clean && yarn build",
        "prestorybook": "[ -d ../../node_modules/@storybook/addon-docs ] || ln -sf ../../packages/ui-components/node_modules/@storybook/addon-docs ../../node_modules/@storybook/addon-docs; [ -d ../../node_modules/@storybook/addon-essentials ] || ln -sf ../../packages/ui-components/node_modules/@storybook/addon-essentials ../../node_modules/@storybook/addon-essentials",
        "storybook": "yarn start:tailwind & start-storybook -p 6006"
    },
    "peerDependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    },
    "dependencies": {
        "@tippyjs/react": "^4.1.0",
        "formik": "^2.1.5",
        "initials": "^3.1.0",
        "lodash.get": "^4.4.2",
        "prop-types": "^15.7.2",
        "react-feather": "^2.0.8",
        "tippy.js": "^6.2.6",
        "yup": "^0.29.3"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@stackrox/tailwind-config": "^0.1.0",
        "@storybook/addon-actions": "^6.0.21",
        "@storybook/addon-essentials": "^6.0.21",
        "@storybook/addon-links": "^6.0.26",
        "@storybook/react": "^6.0.21",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^10.4.9",
        "@types/node": "^14.0.11",
        "@types/react": "^16.9.50",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.0.0",
        "babel-loader": "^8.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^7.2.1",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-jest-dom": "^2.1.0",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "eslint-plugin-testing-library": "^3.1.4",
        "jest": "^24.9.0",
        "jest-junit": "^10.0.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.2",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-is": "^16.13.1",
        "tailwindcss": "^1.8.10",
        "ts-jest": "^24.3.0",
        "typescript": "^3.9.5"
    }
}
