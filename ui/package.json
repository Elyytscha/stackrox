    "scripts": {
        "start": "yarn tailwind && HTTPS=true react-scripts start",
        "start:watch": "npm-run-all --parallel watch:tailwind watch:react",
        "start:coverage": "yarn tailwind && HTTPS=true react-scripts -r @cypress/instrument-cra start",
        "build": "yarn tailwind && GENERATE_SOURCEMAP=false react-scripts build",
        "test": "react-scripts test --coverage",
        "test-watch": "react-scripts test",
        "lint": "yarn tailwind && NODE_PATH=src eslint src cypress",
        "cypress-open": "./scripts/cypress.sh open",
        "cypress-demo-open": "./scripts/cypress.sh open --config integrationFolder=cypress/demo",
        "test-e2e-local": "./scripts/cypress.sh run",
        "test-e2e-local:coverage": "CYPRESS_COLLECT_COVERAGE=true yarn test-e2e-local",
        "test-e2e-ci": "./scripts/cypress.sh run --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha.config.json",
        "test-e2e-ci:coverage": "CYPRESS_COLLECT_COVERAGE=true yarn test-e2e-ci",
        "test-e2e-demo-local": "./scripts/cypress.sh run --config integrationFolder=cypress/demo",
        "test-e2e-demo-ci": "./scripts/cypress.sh run --config integrationFolder=cypress/demo --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha.config.json",
        "connect": "../scripts/connect-ui.sh ${1}",
        "deploy": "../deploy/k8s/deploy.sh",
        "deploy-local": "../deploy/k8s/deploy-local.sh",
        "storybook": "start-storybook",
        "build-storybook": "build-storybook -c .storybook",
        "tailwind": "tailwind build ./src/app.css -o ./src/index.css",
        "watch:tailwind": "postcss ./src/app.css -o ./src/index.css -w",
        "watch:react": "HTTPS=true react-scripts start",
        "forward": "../scripts/port-forward-ui.sh",
        "eject": "react-scripts eject"
    },
