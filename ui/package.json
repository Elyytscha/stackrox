  "scripts": {
    "start": "tailwind build ./src/app.css -c ./tailwind.js -o ./src/index.css && HTTPS=true NODE_PATH=src react-scripts start",
    "build": "tailwind build ./src/app.css -c ./tailwind.js -o ./src/index.css && NODE_PATH=src react-scripts build",
    "test": "NODE_PATH=src react-scripts test --env=jsdom --coverage",
    "test-watch": "NODE_PATH=src react-scripts test --env=jsdom",
    "lint": "NODE_PATH=src eslint src cypress",
    "cypress-open": "cypress open",
    "test-e2e-local": "cypress run",
    "test-e2e-ci": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha.config.json",
    "eject": "react-scripts eject",
    "postinstall": "node ./scripts/spdy-issue47-quickfix.js",
    "connect": "../scripts/connect-ui.sh ${1}",
    "deploy": "../deploy/k8s/deploy.sh",
    "forward": "../scripts/port-forward-ui.sh"
  },
  "proxy": {
    "^/v1": {
      "target": "https://localhost:8000",
      "changeOrigin": true,
      "secure": false
    },
    "^/api": {
      "target": "https://localhost:8000",
      "changeOrigin": true,
      "secure": false
    },
    "^/sso": {
      "target": "https://localhost:8000",
      "changeOrigin": true,
      "secure": false,
      "xfwd": true
    }
  },
  "devDependencies": {
